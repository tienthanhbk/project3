<% extend('../layout/master') %>

    <div ng-app="admin">
        <div ng-controller="manage_categories">
            <div id="page-inner">
                <div class="row">
                    <div class="col-md-12">
                        <h1 class="page-header">
                            Tables Page <small>Responsive tables</small>
                        </h1>
                    </div>
                </div>
                <!-- /. ROW  -->

                <div class="row">
                    <div class="col-md-12">
                        <!-- Advanced Tables -->
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                Categories
                            </div>
                            <div>
                                <form>
                                    <input type="text" placeholder="Name new category" ng-model="nameCategory">
                                    <button ng-click="createCategory()">Create New Category</button>
                                </form>
                            </div>
                            <div class="panel-body">
                                <div class="table-responsive">
                                    <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                                        <thead>
                                            <tr>
                                                <th>ID</th>
                                                <th>Category</th>
                                                <th>Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr class="gradeU" ng-repeat="x in listCate">
                                                <td>{{x.idCategory}}</td>
                                                <td>{{x.name}}</td>
                                                <td>
                                                    <button ng-click="delete(x.idCategory)">delete</button>
                                                    <button>detail</button>
                                                    <button ng-click="edit(x.idCategory)">edit</button>
                                                </td>

                                            </tr>
                                        </tbody>
                                    </table>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
